{
  "name": "treesift",
  "version": "1.0.0",
  "description": "A powerful JavaScript/TypeScript AST analysis library for React components and beyond",
  "license": "MIT",
  "author": "Preston Arnold",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "dependencies": {
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@babel/types": "^7.27.0",
    "chalk": "^5.4.1"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.7",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.30.0",
    "@typescript-eslint/parser": "^8.30.0",
    "eslint": "^9.24.0",
    "prettier": "^3.2.5",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "ast",
    "analysis",
    "react",
    "typescript",
    "javascript",
    "parser",
    "components",
    "documentation",
    "static-analysis"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prestons18/treesift.git"
  },
  "bugs": {
    "url": "https://github.com/prestons18/treesift/issues"
  },
  "homepage": "https://github.com/prestons18/treesift#readme"
}
